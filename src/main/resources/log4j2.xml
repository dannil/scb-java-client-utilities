<?xml version="1.0" encoding="UTF-8"?>
<Configuration package="com.github.dannil.scbjavaclient"
  status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="[%p] %d{yyyy-MM-dd HH:mm:ss zzz} %c{1}:%L - %m%n" />
    </Console>
  </Appenders>
  <Loggers>
    <Root level="debug">
      <AppenderRef ref="Console" />
    </Root>
  </Loggers>
</Configuration>